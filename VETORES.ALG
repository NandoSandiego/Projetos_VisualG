algoritmo "VETORES"

// v: vetor [1..4]:de inteiro
//Inicio
// n[1] <- 3
// n[2] <- 6
// n[3] <- 21
// n[4] <- 7

var

num: vetor[1..5] de real
i : inteiro



inicio


Para i <- 1 ate 5 faca
     Escreva("Digite o ",i,"º valor :")
     leia (num[i])
     EscrevaL("num[",i,"]<-",num[i])
FimPara




fimalgoritmo